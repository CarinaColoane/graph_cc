    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/balbudgt -->


    <owl:Class rdf:about="http://www.constituteproject.org/ontology/balbudgt">
        <rdfs:subClassOf rdf:resource="http://www.constituteproject.org/ontology/Special_Legislation"/>
        <terms:description xml:lang="en">
   A requirement that the government pass a budget in which incoming revenue meets or exceeds the value of the government&apos;s expenditures in a fiscal year.
  </terms:description>
        <terms:description xml:lang="es">
   El requisito de que el gobierno debe aprobar un presupuesto en el que los ingresos entrantes sean iguales o mayores que el monto de los gastos en un año fiscal.
  </terms:description>
        <ontology:keyword xml:lang="en">
   fiscal policy
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   política fiscal
  </ontology:keyword>
        <rdfs:label xml:lang="en">
   Balanced budget
  </rdfs:label>
        <rdfs:label xml:lang="es">
   Presupuesto equilibrado
  </rdfs:label>
    </owl:Class>
    


