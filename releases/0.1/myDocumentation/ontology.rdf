  </terms:description>
        <ontology:keyword xml:lang="en">
   inhumane treatment
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   trato inhumano
  </ontology:keyword>
        <rdfs:label xml:lang="es">
   Prohibición de tratos crueles
  </rdfs:label>
        <rdfs:label xml:lang="en">
   Prohibition of cruel treatment
  </rdfs:label>
    </owl:Class>
    


