  </rdfs:label>
        <rdfs:label xml:lang="es">
   Nombramiento del jefe de las fuerzas armadas
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/commit -->


    <owl:Class rdf:about="http://www.constituteproject.org/ontology/commit">
