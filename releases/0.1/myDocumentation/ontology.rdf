   criminal procedure
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   procedimiento penal
  </ontology:keyword>
        <ontology:keyword xml:lang="ar">
   المتهم
  </ontology:keyword>
        <rdfs:label xml:lang="es">
   Derecho a un abogado
  </rdfs:label>
        <rdfs:label xml:lang="en">
   Right to counsel
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/cruelty -->


