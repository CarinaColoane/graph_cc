  </rdfs:label>
        <rdfs:label xml:lang="en">
   Term limits for first chamber
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/libel -->


    <owl:Class rdf:about="http://www.constituteproject.org/ontology/libel">
