   divorce
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   familia
  </ontology:keyword>
        <ontology:keyword xml:lang="en">
   family
  </ontology:keyword>
        <ontology:keyword xml:lang="ar">
   طلاق
  </ontology:keyword>
        <rdfs:label xml:lang="es">
   Derecho al matrimonio
  </rdfs:label>
        <rdfs:label xml:lang="en">
   Regulation of marriage
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/matequal -->


    <owl:Class rdf:about="http://www.constituteproject.org/ontology/matequal">
        <rdfs:subClassOf rdf:resource="http://www.constituteproject.org/ontology/Minority_Rights"/>
