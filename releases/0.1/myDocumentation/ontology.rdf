  </terms:description>
        <ontology:keyword xml:lang="en">
   fiscal policy
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   política fiscal
  </ontology:keyword>
        <rdfs:label xml:lang="en">
   Budget bills
  </rdfs:label>
        <rdfs:label xml:lang="es">
   Proyectos legislativos presupuestarios
  </rdfs:label>
    </owl:Class>
    


