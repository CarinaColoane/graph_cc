  </ontology:keyword>
        <ontology:keyword xml:lang="en">
   family
  </ontology:keyword>
        <ontology:keyword xml:lang="en">
   gender
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   género
  </ontology:keyword>
        <ontology:keyword xml:lang="en">
   women&apos;s rights
  </ontology:keyword>
        <ontology:keyword xml:lang="ar">
   العائلة
  </ontology:keyword>
        <rdfs:label xml:lang="es">
   Igualdad marital
  </rdfs:label>
        <rdfs:label xml:lang="en">
   Provision for matrimonial equality
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/medcom -->


    <owl:Class rdf:about="http://www.constituteproject.org/ontology/medcom">
