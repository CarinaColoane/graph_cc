  </ontology:keyword>
        <ontology:keyword xml:lang="en">
   voting
  </ontology:keyword>
        <ontology:keyword xml:lang="es">
   voto
  </ontology:keyword>
        <ontology:keyword xml:lang="ar">
   تصويت
  </ontology:keyword>
        <rdfs:label xml:lang="es">
   Restricciones al voto
  </rdfs:label>
        <rdfs:label xml:lang="en">
   Restrictions on voting
  </rdfs:label>
    </owl:Class>
    


    <!-- http://www.constituteproject.org/ontology/voteun -->


